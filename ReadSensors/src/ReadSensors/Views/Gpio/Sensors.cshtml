@model dynamic

@{
    ViewBag.Title = "Gpio Test";
    Layout = "/Views/Shared/_Layout.cshtml";
}



<div class="row" ng-controller="SensorsController as gpio" style="margin-top: 40px;">
    <div class="col-md-12">
        <h2>@ViewBag.Title</h2>
        <p>Status: {{gpio.status}}</p>
        <p><button id="enable" ng-click="gpio.enable()" type="button">Enable Accelerator</button></p>
        <p><button id="disable" ng-click="gpio.disable()" type="button">Disable Accelerator</button></p>
        <p>
            Data:<br/>
            {{gpio.data}}
        </p>
    </div>
</div>
